{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1",
    "react-scripts": "2.0.5",
    "tailwindcss": "^0.7.2"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css --watch",
    "react-dev-server": "react-scripts start",
    "start": "npm-run-all --parallel watch:css react-dev-server",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
